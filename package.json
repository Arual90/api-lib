{
  "name": "api-lib",
  "version": "0.0.1",
  "description": "Simple and modular api calls",
  "main": "index.js",
  "author": "Laura Santalucia",
  "license": "MIT",
  "scripts": {
    "test": "nyc ava",
    "flow-copy": "flow-copy-source -v src/ flow-typed/"
  },
  "dependencies": {
    "ava": "0.24.0",
    "babel-core": "6.26.0",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "data.maybe": "1.2.2",
    "flow-bin": "0.63.1",
    "flow-copy-source": "1.2.1",
    "nyc": "11.4.1",
    "redux": "3.7.2",
    "sinon": "4.1.4",
    "whatwg-fetch": "2.0.3"
  },
  "babel": {
    "comments": false,
    "compact": true,
    "presets": [
      "flow",
      "env"
    ],
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "files": [
      "./src/**/test/*.js"
    ],
    "failFast": true,
    "failWithouAssertions": false,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}