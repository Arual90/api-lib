{
  "name": "api-lib",
  "version": "0.0.1",
  "description": "Simple and modular api calls",
  "main": "index.js",
  "author": "Laura Santalucia",
  "license": "MIT",
  "scripts": {
    "flow": "flow",
    "test": "nyc ava"
  },
  "dependencies": {
    "ava": "0.24.0",
    "babel-core": "6.26.0",
    "babel-plugin-ramda": "1.4.3",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "data.maybe": "1.2.2",
    "flow-bin": "0.63.1",
    "nyc": "11.4.1",
    "sinon": "4.1.4",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "comments": false,
    "compact": true,
    "presets": [
      "flow",
      "es2015"
    ],
    "plugins": [
      "ramda",
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "files": [
      "./src/**/test/*.js"
    ],
    "failFast": true,
    "failWithouAssertions": false,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}